import urllib.request
from bs4 import BeautifulSoup
import re
site = urllib.request.urlopen('https://duckduckgo.com/?q=hi')
data = site.read()
parsed = BeautifulSoup(data)
count = 0
for i in parsed.findAll('div', {'class': re.compile('links_main*')}):
    print (i.a['href'])
    count = count +1
